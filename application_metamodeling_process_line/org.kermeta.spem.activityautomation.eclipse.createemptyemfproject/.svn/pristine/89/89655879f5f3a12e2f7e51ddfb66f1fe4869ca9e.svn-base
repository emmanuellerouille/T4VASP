    <modelVersion>4.0.0</modelVersion>
    <groupId>org.kermeta.spem.activityautomation.eclipse</groupId>
    <artifactId>spem.activityautomation.eclipse.createemptyemfproject</artifactId>
    <version>1.9.9-SNAPSHOT</version>
    <name>spem.activityautomation.eclipse.createemptyemfproject</name>
    <packaging>bundle</packaging>
   
    <parent>
        <groupId>org.kermeta.language</groupId>
        <artifactId>language.root</artifactId>
        <version>2.0.3-SNAPSHOT</version>
    </parent>
   
    <build>
        <sourceDirectory>src/main/java</sourceDirectory>
        <resources>
            <!-- <resource>
                <filtering>false</filtering>
                <directory>src/main/java</directory>
            </resource> -->
            <resource>
                <directory>.</directory>
                <includes>
                    <include>plugin.properties</include>
                    <include>plugin.xml</include>
                </includes>
            </resource>
            <resource>
                <directory>org.kermeta.spem.activityautomation.eclipse.createemptyemfproject</directory>
                <targetPath>src/main/java/org.kermeta.spem.activityautomation.eclipse.createemptyemfproject</targetPath>
            </resource>
        </resources>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <argLine>-Xmx1024m</argLine>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>

            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <extensions>true</extensions>

                <configuration>
                    <instructions>
                        <Bundle-SymbolicName>org.kermeta.${project.artifactId};singleton:=true</Bundle-SymbolicName>
                        <Bundle-ManifestVersion>2</Bundle-ManifestVersion>
                        <Bundle-Version>${bundle.version}</Bundle-Version>
                        <Export-Package>org.kermeta.spem.*</Export-Package>
                        <Import-Package>
                            !*
                        </Import-Package>
                        <Require-Bundle>org.eclipse.core.runtime,org.eclipse.emf.ecore,org.eclipse.emf.codegen.ecore,org.eclipse.core.resources,org.eclipse.ui,org.eclipse.emf.common,org.eclipse.emf.ecore.xmi,org.eclipse.emf.edit.ui,org.eclipse.emf.codegen.ecore.ui,org.eclipse.emf.codegen.ui,org.eclipse.emf.ecore.editor
                        </Require-Bundle>
                        <Bundle-ClassPath>.</Bundle-ClassPath>
                    </instructions>
                </configuration>
            </plugin>
            <!-- copy generated MANIFEST in the project for eclipse user -->
             <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>truezip-maven-plugin</artifactId>
                <version>1.0</version>
                <executions>
                    <execution>
                        <id>copy-out-files</id>
                        <goals>
                            <goal>copy</goal>
                        </goals>
                        <phase>install</phase>
                        <configuration>
                            <files>
                                <file>
                                    <source>${project.build.outputDirectory}/META-INF/MANIFEST.MF</source>
                                    <outputDirectory>${project.basedir}/META-INF</outputDirectory>
                                </file>
                            </files>
                        </configuration>
                    </execution>

                </executions>
            </plugin>
        </plugins>
    </build>
