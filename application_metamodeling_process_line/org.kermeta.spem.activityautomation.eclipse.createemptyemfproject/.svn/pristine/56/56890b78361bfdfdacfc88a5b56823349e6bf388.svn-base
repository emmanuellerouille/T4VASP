package org.kermeta.spem.activityautomation.eclipse.createemptyemfproject;

import org.eclipse.ui.PlatformUI;

import org.kermeta.spem.activityautomation.eclipse.createemptyemfproject.CreateEmptyEMFProject;

public class StaticCall {

	public static void createEmptyEMFProject(String contextFilePath){
		CreateEmptyEMFProject createEmptyEMFProject = new CreateEmptyEMFProject();
		createEmptyEMFProject.run(PlatformUI.getWorkbench().getActiveWorkbenchWindow(), contextFilePath);
	}
}
