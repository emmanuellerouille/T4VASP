<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.kermeta.spem.executioncontext.presentation.ExecutioncontextModelWizardID"
            name="%_UI_ExecutioncontextModelWizard_label"
            class="org.kermeta.spem.executioncontext.presentation.ExecutioncontextModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ExecutioncontextModelFile.gif">
         <description>%_UI_ExecutioncontextModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.kermeta.spem.executioncontext.presentation.ExecutioncontextEditorID"
            name="%_UI_ExecutioncontextEditor_label"
            icon="icons/full/obj16/ExecutioncontextModelFile.gif"
            extensions="executioncontext"
            class="org.kermeta.spem.executioncontext.presentation.ExecutioncontextEditor"
            contributorClass="org.kermeta.spem.executioncontext.presentation.ExecutioncontextActionBarContributor">
      </editor>
   </extension>

</plugin>
