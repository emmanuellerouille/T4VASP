<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.kermeta.spem.behavior.presentation.BehaviorModelWizardID"
            name="%_UI_BehaviorModelWizard_label"
            class="org.kermeta.spem.behavior.presentation.BehaviorModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/BehaviorModelFile.gif">
         <description>%_UI_BehaviorModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.kermeta.spem.behavior.presentation.BehaviorEditorID"
            name="%_UI_BehaviorEditor_label"
            icon="icons/full/obj16/BehaviorModelFile.gif"
            extensions="behavior"
            class="org.kermeta.spem.behavior.presentation.BehaviorEditor"
            contributorClass="org.kermeta.spem.behavior.presentation.BehaviorActionBarContributor">
      </editor>
   </extension>

</plugin>
