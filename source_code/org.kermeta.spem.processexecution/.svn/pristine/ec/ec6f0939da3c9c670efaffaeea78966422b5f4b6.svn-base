package org.kermeta.spem.processexecution.model;

import org.kermeta.spem.processexecution.model.SPEMDeliveryProcess;

public class SPEMTaskUse {
	
	private String name;
	private String deliveryProcessPath;
	private SPEMDeliveryProcess deliveryProcess;

	public SPEMTaskUse(String name, String deliveryProcessPath) {
		this.name = name;
		this.deliveryProcessPath = deliveryProcessPath;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDeliveryProcessPath() {
		return deliveryProcessPath;
	}

	public void setDeliveryProcessPath(String deliveryProcessPath) {
		this.deliveryProcessPath = deliveryProcessPath;
	}

	public SPEMDeliveryProcess getDeliveryProcess() {
		return deliveryProcess;
	}

	public void setDeliveryProcess(SPEMDeliveryProcess deliveryProcess) {
		this.deliveryProcess = deliveryProcess;
	}

}
